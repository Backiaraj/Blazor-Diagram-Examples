    <SfDiagramComponent @ref="diagram" Width="1000px" NodeCreating="@NodeCreating" ConnectorCreating="@ConnectorCreating" Height="1000px"    SelectionSettings="@select" >
        <DiagramTemplates>
            <UserHandleTemplate >
                @{
                       
                        
