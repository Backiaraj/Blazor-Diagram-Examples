                    <PopupTemplate>
                        <div class="popuptempContent">
                               
                                <div class="content">
                                
                                    
                                      <SfColorPicker Inline="true"  ValueChange="OnFillColorChange" @bind-Value="@fillColor"></SfColorPicker>
                                  
                                </div>
                            
                        </div>
                    </PopupTemplate>
                </SfSpeedDial>
                       
                  }
                                  @if(item.ID=="cut1")
